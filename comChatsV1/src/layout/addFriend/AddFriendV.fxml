<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" prefHeight="62.0" prefWidth="320.0" style="-fx-background-color: #2A363F;"
      stylesheets="@AddFriend.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="layout.addFriend.AddFriendC">
    <children>
        <HBox alignment="CENTER" prefHeight="42.0" prefWidth="279.0" spacing="5.0" VBox.vgrow="ALWAYS">
            <children>
                <TextField fx:id="name" prefHeight="33.0" prefWidth="222.0" promptText="enter name"
                           HBox.hgrow="ALWAYS"/>
                <Button defaultButton="true" onAction="#add" prefHeight="33.0" prefWidth="76.0" text="add"
                        HBox.hgrow="ALWAYS"/>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
